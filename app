2. import React from 'react';
import data from './data';



function App() {
  return (
    <div className="grid-container">
    <header className="header">
      <div className="brand">
        <button onclick="openMenu()">
          &#9776;
        </button>
        <a href="index.html">Shop4u</a>
      </div>
      <div className="header-links">
        <a href="cart.html">Cart</a>
        <a href="signin.html">Sign In</a>
      </div>
    </header>
    <aside className="sidebar">
      <h3>Shopping Categories</h3>
      <button className="sidebar-close-button" onclick="closeMenu()">x</button>
      <ul>
        <li>
          <a href="index.html">Shirt</a>
        </li>

        <li>
          <a href="index.html">Pants</a>
        </li>

        <li>
            <a href="index.html">Inner-Wear</a>
        </li>
        
        <li>
            <a href="index.html">Traditional Wear</a>
        </li>
        
        <li>
            <a href="index.html">Leisure Wear</a>
        </li>

        <li>
            <a href="index.html">Suitings</a>
        </li>

      </ul>
    </aside>
    <main className="main">
      <div className="content">
        {
          data.products.map(product =>{
            return (
              <div className="product">
                <img className="product-image" src="images/fs1.jpeg" alt="product" />
                <div className="product-name">
                  <a href="product.html">{product.name}</a>
                </div>
                <div className="product-brand">Cool Colors</div>
                <div className="product-price">₹1460</div>
                <div className="product-rating">4.8 Stars (75 Reviews)</div>
              </div>
            );
          }
        }
        
      </div>

    </main>
    <footer className="footer">
      All right reserved.
    </footer>
  </div>

  );
}

export default App;










































